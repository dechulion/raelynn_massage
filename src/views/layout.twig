  </head>
  <body>

  <!-- Header Start -->
  <header data-spy="affix" data-offset-top="210">

    <div class="container">
      <div class="header-content">

        <!-- header top start -->
        <div class="row-fluid header-top">


          <!-- Author Infos Start -->
          <div class="span7 left-shadow">


            <!-- find me button Start -->
            <div id="find_me">
              <div id="button-shadow"></div>
              <a><i class="icon-location-arrow"></i> Find Me</a>
            </div>
            <!-- find me button end -->


            <!-- author photo Start -->
            <div class="pull-left author-image-column">
              <div class="author-background"></div>
              <img src="http://placehold.it/82x98/464445/cccccc.png" id="author-image" alt="Linda Sylvain" />
            </div>
            <!-- author photo end -->


            <!-- author name and description Start -->
            <div class="pull-left author-infos-column">

              <div class="author-info">
                Linda Sylvain
                <span class="author-work">/ Grafic Desinger</span>
              </div>

              <div class="hr"></div>

              <div class="author-about">
                I like to make cool and creative designs.
                Feel free to take a look My template.
              </div>

            </div>
            <div class="clearfix"></div>
            <!-- author name and description end -->


          </div>
          <!-- Author Infos End -->


          <!-- Contact Infos Start -->
          <div class="span5 text-right" id="contact">

            <div>
              <i class="icon-phone"></i> 555-787-121
            </div>

            <div class="address">
              <i class="icon-map-marker"></i> 55 Sesame street, Lorem Ipsum
            </div>

            <div class="hr hr_right"></div>

            <div class="email">
              <a href="mailto:hi@zaferyardimci.com">
                <i class="icon-envelope"></i> hi@zaferyardimci.com
              </a>
            </div>

          </div>
          <div class="clearfix"></div>
          <!-- Contact Infos End -->


          <!-- Google Map Area Start -->
          <div id="header_opened"></div>
          <!-- Google Map Area End -->

        </div> <!-- header top end -->



        <!-- Search Form Start -->
        <form method="post" id="search-form">
          <input placeholder="Search.." type="text" />
        </form>
        <!-- Search Form End -->



        <!-- header bottom start -->
        <div class="row-fluid header-bottom">


          <!-- breadcrumb Start -->
          <div class="pull-left">
            <div class="custom-breadcrumb">
              <a href="#">Home</a> <i class="icon-double-angle-right"></i> Page
            </div>
          </div>
          <!-- breadcrumb End -->


          <!-- Social Icons Start -->
          <div class="pull-right text-right">
            <a href="#" id="twitter-icon"></a>
            <a href="#" id="facebook-icon"></a>
            <a href="#" id="rss-icon"></a>
            <a href="#" id="search-icon"></a>
          </div>
          <!-- Social Icons End -->

          <div class="clearfix"></div>


        </div> <!-- header bottom End -->


      </div> <!-- .header-content End -->
    </div> <!-- .container End -->
  </header>
  <!-- Header End -->


  <!-- Main container Start -->
  <div class="container main-container">

    <!-- Nav Start -->
    <nav>
      <div class="affix nav-affix">

        <!-- Logo Start -->
        <div id="logo">
          <a href="index.html">
            <img src="img/logo.png" alt="Chloe" />
          </a>
        </div>
        <!-- Logo End -->


        <!-- Menu Start -->
        <ul class="menu">

          <li><a href="index.html"><i class="icon-home"></i> Home</a></li>

          <li>
            <a href="#"><i class="icon-pencil"></i> Blog</a>
            <ul>
              <li><a href="blog.html"><i class="icon-pencil"></i> Blog</a></li>
              <li><a href="single-image.html"><i class="icon-picture"></i> Image Post</a></li>
              <li><a href="single-video.html"><i class="icon-play-sign"></i> Video Post</a></li>
              <li><a href="single-sound.html"><i class="icon-music"></i> Sound Post</a></li>
              <li><a href="single-quote.html"><i class="icon-quote-left"></i> Quote Post</a></li>
            </ul>
          </li>

          <li><a href="#"><i class="icon-briefcase"></i> Portfolio</a>
            <ul>
              <li><a href="portfolio.html"><i class="icon-briefcase"></i> Portfolio</a></li>
              <li><a href="item.html"><i class="icon-star"></i> Portfolio Item</a></li>
            </ul>
          </li>

          <li class="active"><a href="#"><i class="icon-rocket"></i> Features</a>
            <ul>
              <li><a href="typography.html"><i class="icon-font"></i> Typography</a></li>
              <li class="active"><a href="page.html"><i class="icon-file-text"></i> Sample Page</a></li>
            </ul>
          </li>

          <li><a href="contact.html"><i class="icon-phone-sign"></i> Contact Me</a></li>

        </ul>
        <!-- Menu End -->


      </div>
    </nav>
    <!-- Nav End -->


    <!-- Section Start -->
    <section>

      <!-- Page start -->
      <article>

        <h2 class="page-heading">{% block pagetitle %}{% endblock %}</h2>
        {% block content %}{% endblock %}


      </article>
      <!-- Page end -->

    </section>
    <!-- Section End -->



    <!-- footer Start -->
    <footer>
      <div class="row-fluid">

        <div class="span6">
          &copy; Copyright 2013. All Rights Reserved.
        </div>

        <div class="span6 text-right">
          Coded and designed by <a href="http://www.zaferyardimci.com" target="_blank">Zafer Yardimci</a>
        </div>

      </div>
    </footer>
    <!-- footer End -->


  </div><!-- Main container End -->


  <!-- Libs -->
  <script src="js/jquery.js"></script>
  <script src="js/bootstrap.js"></script>
  <script src="js/colorbox.js"></script>

  <!-- include Caroufredsel plugin -->
  <script type="text/javascript" src="js/carouFredSel.js"></script>
  <script type="text/javascript" src="js/touchSwipe.js"></script>

  <!-- include Portfolio filter plugin -->
  <script type="text/javascript" src="js/mixitup.js"></script>


  <!-- theme scripts -->
  <script src="js/scripts.js"></script>

  <!-- custom.js for you can add your custom codes -->
  <script src="js/custom.js"></script>


  </body>
</html>